.photo
	box-sizing content-box
	width 233px
	height 350px
	font-size 0
	transform rotate(-2.5deg)
	border 5px solid $bg-workspace
	&::before
		content ''
		position absolute
		left -15px
		bottom -15px
		width 50px
		height 50px
		backImage(corner)
		transform rotate(-90deg)
	&::after
		content ''
		position absolute
		right -15px
		top -15px
		width 50px
		height 50px
		backImage(corner)
		transform rotate(90deg)
	&__picture
		display none
	@supports (filter sepia(0.8))
		filter sepia(0.8)
		&__svg
			display none
		&__picture
			display block
